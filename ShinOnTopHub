-- ðŸŒ¸ Project Reverse GUI - Stylish Pink by ShinOnTop

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Title = Instance.new("TextLabel")
local MinimizeButton = Instance.new("TextButton")
local ButtonHolder = Instance.new("Frame")
local UIListLayout = Instance.new("UIListLayout")
local Watermark = Instance.new("TextLabel")

local function createButton(name, callback)
    local btn = Instance.new("TextButton")
    btn.Text = name
    btn.Size = UDim2.new(1, -10, 0, 40)
    btn.BackgroundColor3 = Color3.fromRGB(255, 170, 200)
    btn.BorderSizePixel = 0
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.Parent = ButtonHolder
    btn.MouseButton1Click:Connect(callback)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
end

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 105, 180)
Frame.Size = UDim2.new(0, 310, 0, 440)
Frame.Position = UDim2.new(0.3, 0, 0.2, 0)
Frame.Active = true
Frame.Draggable = true
UICorner.Parent = Frame
UICorner.CornerRadius = UDim.new(0, 10)

Title.Parent = Frame
Title.BackgroundTransparency = 1
Title.Position = UDim2.new(0, 10, 0, 5)
Title.Size = UDim2.new(1, -40, 0, 30)
Title.Text = "ðŸŒ¸ Project Reverse GUI"
Title.TextColor3 = Color3.fromRGB(255, 245, 255)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 18
Title.TextXAlignment = Enum.TextXAlignment.Left

MinimizeButton.Parent = Frame
MinimizeButton.Size = UDim2.new(0, 30, 0, 30)
MinimizeButton.Position = UDim2.new(1, -35, 0, 5)
MinimizeButton.BackgroundColor3 = Color3.fromRGB(255, 128, 190)
MinimizeButton.Text = "-"
MinimizeButton.Font = Enum.Font.GothamBold
MinimizeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
MinimizeButton.TextSize = 20

local isMinimized = false
MinimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    ButtonHolder.Visible = not isMinimized
    Watermark.Visible = not isMinimized
end)

ButtonHolder.Parent = Frame
ButtonHolder.BackgroundTransparency = 1
ButtonHolder.Position = UDim2.new(0, 10, 0, 40)
ButtonHolder.Size = UDim2.new(1, -20, 1, -150)
UIListLayout.Parent = ButtonHolder
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 8)

Watermark.Parent = Frame
Watermark.BackgroundTransparency = 1
Watermark.Position = UDim2.new(0, 10, 1, -35)
Watermark.Size = UDim2.new(1, -20, 0, 20)
Watermark.Text = "ShinOnTop GUI"
Watermark.TextColor3 = Color3.fromRGB(255, 200, 250)
Watermark.Font = Enum.Font.Gotham
Watermark.TextSize = 12
Watermark.TextXAlignment = Enum.TextXAlignment.Right

